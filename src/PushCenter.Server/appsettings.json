{
  "ConnectionStrings": {
    "PushCenter": "host=localhost;database=pushcenter;user id=pushcenter;password=pushcenter;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "pushcenter"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "OutputTemplate": "{Timestamp:HH:mm:ss} {Level:u3} [{SourceContext}]: {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "PushCenter": {
    "PushApi": {
      "Subject": "",
      "PublicKey": "",
      "PrivateKey": ""
    }
  }
}